{
	"_version": "1.9.0",
	"sap.app": {
		"id": "sitbh",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/ODATA_SAMPLE/V2/(S(fabiopagoti))/OData/OData.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sitbh.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.42.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sitbh.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"disableHeadRequestForToken": true,
					"defaultBindingMode": "TwoWay"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sitbh.view",
				"controlId": "mainContainer",
				"controlAggregation": "pages",
				"transition": "flip"
			},
			"routes": [{
				"pattern": "",
				"name": "default",
				"target": ["categorias"]
			}, {
				"name": "produtos",
				"pattern": "/Categorias/{idCategoria}",
				"target": ["produtos"]
			}, {
				"name": "novaCategoria",
				"pattern": "/NovaCategoria",
				"target": ["cadastro"]
			}],
			"targets": {
				"categorias": {
					"viewName": "Categorias",
					"viewLevel": 1
				},
				"produtos": {
					"viewName": "Produtos",
					"viewLevel": 2
				},
				"cadastro": {
					"viewName": "NovaCategoria",
					"viewLevel": 2
				}
			}
		}
	}
}